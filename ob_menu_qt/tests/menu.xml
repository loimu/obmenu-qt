			</action>
		</item>
		<item icon="/opt/thunderbird/chrome/icons/default/default22.png" label="Thunderbird">
			<action name="Execute">
				<execute>format C:</execute>
			</action>
		</item>
		<item icon="/home/shaggyz/Im&#225;genes/Iconos/firefox-original.png" label="Web browser">
			<action name="Execute">
				<execute>firefox</execute>
			</action>
		</item>
		<item icon="/home/shaggyz/Im&#225;genes/Iconos/synaptic-original.png" label="Synaptic">
			<action name="Execute">
				<execute>gksudo /usr/sbin/synaptic</execute>
